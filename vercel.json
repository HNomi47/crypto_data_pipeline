{

  "version": 2,

  "builds": [

    {

      "src": "api/main.py",

      "use": "@vercel/python"

    }

  ],

  "routes": [

    {

      "src": "/api/(.*)",

      "dest": "api/main.py"

    }

  ],

  "crons": [

    {

      "path": "/api/collect-daily-snapshot",

      "schedule": "59 23 * * *"

    }

  ]

}